{
  "name": "svelte-toolkit-docs",
  "description": "Documentation for svelte-toolkit, a collection of UI components for Svelte",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "export": "sapper export --basepath svelte-toolkit-docs",
    "move": "rimraf docs && move-cli __sapper__/export/svelte-toolkit-docs docs",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "normalize.css": "^8.0.1",
    "polka": "^0.5.2",
    "sirv": "^0.4.2"
  },
  "devDependencies": {
    "css-loader": "^3.5.2",
    "move-cli": "^1.2.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-load-config": "^2.1.0",
    "rimraf": "^3.0.2",
    "sapper": "^0.27.12",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "svelte": "^3.20.1",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.7.3",
    "svelte-toolkit": "^0.3.0",
    "webpack": "^4.43.0"
  }
}
